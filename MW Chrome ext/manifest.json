{
  "name": "EmWave",
  "version": "1.0",
  "description": "Mental wellness check in",
  "background": {
    "scripts": ["popup.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "icons": {"128": "icon_128.png"},
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "storage"]
}
